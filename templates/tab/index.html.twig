{% extends "template.html.twig" %}

{% block body %}
  
{% block title %}
    Tab
{% endblock %}

  <table class="table">
      {% for note in notes %}
      <tr>
          <td>{{note}}</td>
      </tr>
      {% endfor %}
  </table>

  <p>Número de notas: {{notes | length}}</p>
  <p>Nota mínima: {{min(notes)}}</p>
  <p>Nota máxima: {{max(notes)}}</p>

{% endblock %}